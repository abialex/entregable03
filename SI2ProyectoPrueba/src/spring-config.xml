<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="libro" class="pe.edu.unsch.model.Libro">
	<constructor-arg name="idLibros" value="12"></constructor-arg>
	<constructor-arg name="nombres" value="libroNuev"></constructor-arg>
	
	<property name="autor" ref="Autor"></property>
	</bean>
	<bean class="pe.edu.unsch.model.Autor" id="Autor">
		<constructor-arg name="nombre1" value="alex"></constructor-arg>
		<constructor-arg name="apellido1" value="yalle"></constructor-arg>
	</bean>
</beans>
